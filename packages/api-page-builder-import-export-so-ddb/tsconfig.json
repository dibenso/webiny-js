{
  "extends": "../../tsconfig",
  "references": [
    {
      "path": "../db-dynamodb"
    },
    {
      "path": "../error"
    },
    {
      "path": "../api-security"
    },
    {
      "path": "../api-tenancy"
    },
    {
      "path": "../handler"
    },
    {
      "path": "../handler-graphql"
    },
    {
      "path": "../handler-aws"
    },
    {
      "path": "../api-page-builder-import-export"
    }
  ],
  "compilerOptions": {
    "paths": {
      "~/*": [
        "./src/*"
      ],
      "@webiny/api-page-builder-import-export/*": [
        "../api-page-builder-import-export/src/*"
      ],
      "@webiny/api-page-builder-import-export": [
        "../api-page-builder-import-export/src"
      ],
      "@webiny/api-security/*": [
        "../api-security/src/*"
      ],
      "@webiny/api-security": [
        "../api-security/src"
      ],
      "@webiny/api-tenancy/*": [
        "../api-tenancy/src/*"
      ],
      "@webiny/api-tenancy": [
        "../api-tenancy/src"
      ],
      "@webiny/db-dynamodb/*": [
        "../db-dynamodb/src/*"
      ],
      "@webiny/db-dynamodb": [
        "../db-dynamodb/src"
      ],
      "@webiny/handler-aws/*": [
        "../handler-aws/src/*"
      ],
      "@webiny/handler-aws": [
        "../handler-aws/src"
      ],
      "@webiny/handler-graphql/*": [
        "../handler-graphql/src/*"
      ],
      "@webiny/handler-graphql": [
        "../handler-graphql/src"
      ],
      "@webiny/error/*": [
        "../error/src/*"
      ],
      "@webiny/error": [
        "../error/src"
      ],
      "@webiny/handler/*": [
        "../handler/src/*"
      ],
      "@webiny/handler": [
        "../handler/src"
      ]
    },
    "baseUrl": "."
  }
}
